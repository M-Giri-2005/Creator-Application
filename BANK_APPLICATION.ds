			calcuate_the_age as "calcuate the age"
			{
				type =  form
				form = customer_details_master
				record event = on add or edit

				on user input of customer_dob
				{
					actions 
					{
						custom deluge script
						(
											if(input.customer_dob != null)
										{
											if(input.customer_dob.yearsBetween(today) < 18)
											{
												input.customer_age = input.customer_dob.yearsBetween(today);
												input.account_age_type = "CHILD";
											}
											else
											{
												input.customer_age = input.customer_dob.yearsBetween(today);
												input.account_age_type = "ADULT";
												hide parent_number;
												hide parent_name;
												hide parent_details;
											}
										}
										else
										{
											input.customer_age = null;
											input.account_age_type = null;
										}
						)
					}
					actions 
					{
						custom deluge script
						(
											if(input.customer_dob != null)
										{
											if(input.customer_dob >= zoho.currentdate || input.customer_dob.yearsBetween(today) < 1)
											{
												alert "NOT VAILD RESON FOR YOU SELECT FOR FUTURE DATE OR MINIMUM CUSTOMER AGE 1 ";
												input.customer_dob = null;
											}
										}
										else
										{
											input.customer_age = null;
											input.account_age_type = null;
										}
						)
					}
				}

			}
			use_lookup_for_customer_i as "use lookup for customer id"
			{
				type =  form
				form = customer_saving_details_master
				record event = on add or edit

				on user input of customer_ID
				{
					actions 
					{
						custom deluge script
						(
											ver = customer_details_master[ID == input.customer_ID];
										input.customer_age = ver.customer_age;
										input.account_age_type = ver.account_age_type;
										input.Customer_Name = ver.Customer_Name;
										input.customer_email = ver.customer_email;
										input.Customer_address = ver.customer_address;
						)
					}
				}

			}
			generate_the_customer_id_1 as "generate the customer id in RD account"
			{
				type =  form
				form = RD_customer_details_master
				record event = on add

				on success
				{
					actions 
					{
						custom deluge script
						(
											ver = RD_customer_details_master[ID != null];
										input.RD_customer_ID = "RD" + (ver.count_3 + 1);
										ver.count_3=ver.count_3 + 1;
						)
					}
				}

			}
			calculate_the_end_date_fo as "calculate the end date for RD account"
			{
				type =  form
				form = RD_customer_details_master
				record event = on add or edit

				on user input of number_of_month
				{
					actions 
					{
						custom deluge script
						(
											if(input.number_of_month >= 12 && input.number_of_month < 24)
										{
											input.interest_rate = 6;
											ver = input.month_of_installment * input.number_of_month;
											rec = ver * input.interest_rate / 100;
											input.total_month_of_installment = ver + rec;
										}
										else if(input.number_of_month >= 24 && input.number_of_month < 36)
										{
											input.interest_rate = 8;
											ver = input.month_of_installment * input.number_of_month;
											rec = ver * input.interest_rate / 100;
											input.total_month_of_installment = ver + rec;
										}
										else if(input.number_of_month == 36)
										{
											input.interest_rate = 10;
											ver = input.month_of_installment * input.number_of_month;
											rec = ver * input.interest_rate / 100;
											input.total_month_of_installment = ver + rec;
										}
						)
					}
					actions 
					{
						custom deluge script
