											}
											rowLastSeatStyle = "";
											seat_list_desc = thisapp.generate_list(24,{});
											seat_list = seat_list_desc.sort(true);
											input.Seats:ui.add(seat_list);
											add_gap_var = 3;
											for each  row_last_seat in seat_list
											{
												if(row_last_seat.toLong() == add_gap_var)
												{
													rowLastSeatStyle = rowLastSeatStyle + "input[id$=checkboxesEl_zc-Seats_" + row_last_seat + "]+label{margin-right:55px;}";
													add_gap_var = add_gap_var + 6;
												}
											}
											widthlist = {0,106,172,236,300,364,428,492,556,620,684};
											input.Seat_Style = "<style>  .flLeft .tempContDiv .zc-Checkbox { display:block;} .form-field .tempContDiv .choice-table-row {display:none;}.form-field .tempContDiv .choice-table-cell{ display:block;float:left;padding:0px;} .customCheckbox+label, .customRadio+label{ margin-right:0 px;border:none; background:url('https://workdrive.zohoexternal.com/external/ff9b77b8b9ac54d210ce2b1cad2161c81458191f3044922d8d8a281c9ca3d9fc/download?directDownload=true');padding: 12px;background-repeat: no-repeat;   width:27px;  background-position: 15px 0px;} .customCheckbox:checked+label{background:none; border:none;    background-image: url('https://workdrive.zohoexternal.com/external/09af22e4c2922b1b8b68941da15f0241b77c69b6e68b9dce9349e2810c82c8ca/download?directDownload=true');background-repeat: no-repeat;    width: 27px;background-position: 15px 0px;} .customCheckbox:focus+label, .customRadio:focus+label{ border:none;} .choice-label-text { display:none; }  .zc_checkbox:checked + label:after {display: none;} label+label { display:none; }" + bookedSeatStyle + " .form-field .tempContDiv { display: table; Width:428px;} " + rowLastSeatStyle + ".form-group label.zc-Seats-label{display:none;} .choice-table-cell span { padding:5px 0 0 0;} </style>";
