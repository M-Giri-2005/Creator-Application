			Fetch_The_Session_Type as "Fetch The Session Type"
			{
				type =  form
				form = Booking_From
				record event = on add or edit

				on user input of Booking_Details.To_Time
				{
					actions 
					{
						custom deluge script
						(
											if(row.To_Time != null)
										{
											if(row.From_Time > row.To_Time)
											{
												alert "Not Valid Reson For You Enter The Wrong Time Or You Check The From Time";
												row.To_Time="";
											}
										}
										if(row.To_Time != null)
										{
											new = Session_Master[From_Time <= row.From_Time && To_Time >= row.To_Time && Session_Status == "Active"].Session_Time_Type;
											row.Session_Time_Type=new;
										}
										if(row.To_Time != null)
										{
											for each  rec in input.Booking_Details
											{
												if(rec.Number != row.Number && rec.Session == row.Session && rec.From_Time <= row.To_Time && rec.To_Time >= row.To_Time)
												{
													alert "Not Valid Reson For You Enter the Same Date And Same Time In Same Row";
													row.From_Time="";
													row.To_Time="";
												}
											}
										}
										val2 = Booking_Existing_SubFrom[Session == row.Session && From_Time <= row.From_Time && To_Time >= row.From_Time && From_Time <= row.To_Time && To_Time >= row.To_Time].Employee_Name.getAll();
										val = Session_Master[Session_Status == "Active" && From_Time <= row.From_Time && To_Time >= row.From_Time && From_Time <= row.To_Time && To_Time >= row.To_Time].Employee_Master_ID;
										val.removeAll(val2);
										row.Employee_Name:ui.add(val);
										//
						)
					}
				}

			}
