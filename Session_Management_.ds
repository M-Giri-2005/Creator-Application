	reports
	{
		default list All_Session_Masters
		{
			displayName = "All Session Masters"
			show all rows from Session_Master    
			(
				Session_Time_Type as "Session Time Type"
				Session_Status as "Session Status"
				From_Time as "From Time"
				To_Time as "To Time"
				custom action "Change In Active"
				(
	  			workflow = Change_In_Active
				show action in edit record menu = true
				column header = "Change In Active"
				condition = (Session_Status == "Active")
				)
			)
		}
		default list All_Employee_Masters
		{
			displayName = "All Employee Masters"
			show all rows from Employee_Master    
