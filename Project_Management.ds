			Create_Project_ID as "Create Project ID"
			{
				type =  form
				form = Project_Creation
				record event = on add

				on success
				{
					actions 
					{
						custom deluge script
						(
											input.Project_ID = "PRO" + input.Project_Name.subString(0,4) + input.Project_Start_Date.getDay() + input.Client_Name.subString(0,3) + input.Auto_Number;
										var = Map();
										var.put("firstName",input.Client_Name.first_name);
										var.put("lastName",input.Client_Name.last_name);
										var.put("email",input.Client_Email);
										var.put("mobile",input.Client_Phone);
										var2 = zoho.desk.create(60055937116,"contacts",var,"zoho_desk_connection");
										Get_Desk_ID = var2.get("id");
										input.Desk_ID = Get_Desk_ID;
										// 
										// 
										// 
										// 
										// 
										// 
										// 
										// 
										// 
										// 
										ver = zoho.workdrive.createFolder(input.Project_Name + "_" + input.Client_Name.first_name,"qtbolefdaad97ac6046b8a35f7f98087bd0fa","zoho_oauth_connection");
										ver2 = ver.get("data").get("id");
										// 
										if(input.Project_Details.Project_Releated_Details_File != null)
										{
											for each  rec in input.Project_Details
											{
												rec2 = zoho.workdrive.uploadFile(rec.Project_Releated_Details_File,ver2,rec.Project_Releated_Details_File,false,"zoho_oauth_connection");
											}
										}
						)
					}
				}

			}
			validation_For_Start_Date as "validation For Start Date"
			{
				type =  form
				form = Project_Creation
				record event = on add or edit

				on user input of Project_Start_Date
				{
					actions 
					{
						custom deluge script
						(
											if(input.Project_Start_Date != null)
										{
											if(input.Project_Start_Date < zoho.currentdate)
											{
												alert "Not Valid Reson For You Select The Past Date";
												input.Project_Start_Date = null;
											}
										}
										if(input.Project_End_Date != null && input.Project_Start_Date != null)
										{
											input.Total_Days = input.Project_Start_Date.daysBetween(input.Project_End_Date);
										}
						)
					}
				}

			}
			validation_For_End_Date as "validation For End Date"
			{
				type =  form
				form = Project_Creation
				record event = on add or edit

				on user input of Project_End_Date
				{
					actions 
					{
						custom deluge script
						(
											if(input.Project_End_Date != null)
										{
											if(input.Project_Start_Date > input.Project_End_Date)
											{
												alert "Not Valid Reson For You Enter The Past Date";
												input.Project_End_Date = null;
											}
										}
										if(input.Project_Start_Date != null && input.Project_End_Date != null)
										{
											input.Total_Days = input.Project_Start_Date.daysBetween(input.Project_End_Date);
										}
