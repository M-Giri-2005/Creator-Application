											var.PRODUCT_QTY=var.PRODUCT_QTY + rec.return_qty;
										}
										ver = MULT_SALES_DETALIS[ID == input.sales_bill_no];
										for each  k in input.return_MATI_SALES_SUBFORM
										{
											for each  g in ver.MATI_SALES_SUBFORM
											{
												if(g.PRODUCT_NAME == k.PRODUCT_NAME)
												{
													g.PRODUCT_QTY=g.PRODUCT_QTY - k.return_qty;
