			Fetch_Worker_Salary_Detai as "Fetch Worker Salary Details"
			{
				type =  form
				form = Worker_Salary
				record event = on add or edit

				on load
				{
					actions 
					{
						custom deluge script
						(
											var = Worker_Details[ID != null];
										val1 = 0;
										name = 0;
										for each  rec in var
										{
											new = Worker_Salary.Worker_Salary_Details();
											new.Worker_ID=rec.ID;
											new.Worker_Role=rec.Worker_Role;
											value2 = Worker_Attendance[Worker_ID == rec.ID && Salary_Status == "Un Paid"].Total_Working_Time.getAll();
											add_ = 0;
											for each  rec3 in value2
											{
												if(new.Worker_Role.Role == "Software developer")
												{
													add_ = add_ + 400 * rec3.ToNumber();
												}
												if(new.Worker_Role.Role == "Manager")
												{
													add_ = add_ + 500 * rec3.ToNumber();
												}
												if(new.Worker_Role.Role == "Team Leader")
												{
													add_ = add_ + 450 * rec3.ToNumber();
												}
												if(new.Worker_Role.Role == "Accountant")
												{
													add_ = add_ + 300 * rec3.ToNumber();
												}
											}
											new.Total_Salary_Amount=add_;
											input.Worker_Salary_Details.insert(new);
										}
						)
					}
				}

			}
			Create_Salary_Bills as "Create Salary Bills"
			{
				type =  form
