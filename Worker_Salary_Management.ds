			Create_Worker_ID as "Create Worker ID"
			{
				type =  form
				form = Worker_Details
				record event = on add

				on success
				{
					actions 
					{
						custom deluge script
						(
											input.Worker_ID = "WOR" + "_" + zoho.currentdate.getDay() + "_" + input.Worker_Name.subString(0,3) + "_" + input.Worker_Date_Of_Brith.getDay() + "_" + input.Worker_Role.Role.subString(0,3) + input.Auto_Number;
										Worker = Map();
										Worker.put("contact_name",input.Worker_Name.first_name);
										Worker.put("company_name",input.Worker_ID);
										Worker.put("contact_type","vendor");
										Worker_list = List();
										emtymap = Map();
										emtymap.put("email",input.Worker_Email);
										emtymap.put("phone",input.Worker_Phone);
										Worker_list.add(emtymap);
										Worker.put("contact_persons",Worker_list);
										response = invokeurl
										[
											url :"https://www.zohoapis.in/books/v3/contacts?organization_id=60047090525"
											type :POST
											parameters:Worker.tostring()
											connection:"zoho_books_interview"
										];
										// input.Worker_Books_ID=response.get("contact").get("contact_id");
										var = response.get("contact").get("contact_id");
										input.Worker_Books_ID = var;
						)
					}
				}

			}
