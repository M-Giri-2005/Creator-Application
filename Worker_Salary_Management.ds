										for each  rec in new
										{
											new.Salary_Status="Paid";
										}
						)
					}
				}

			}
		}

		schedule
		{
			Inset_Absent_Worker_ as "Inset Absent Worker "
			{
				type =  schedule
				start = "2025-11-17 23:59:00"
				frequency = daily
				time zone = "Asia/Kolkata"
				on start
				{
					actions 
					{
					on load
					(
						new = Worker_Details[ID != null].Worker_ID.getAll();
										var = Worker_Attendance[ID != null && Today_Date == zoho.currentdate].Worker_ID.Worker_ID.getAll();
										new.removeall(var);
										for each  val in new
										{
											res = insert into Worker_Attendance
											[
												Added_User=zoho.loginuser
												Today_Date=zoho.currentdate
												Worker_Name=Worker_Details[Worker_ID == val].Worker_Name
												Check_In_Date_And_Time=null
												Check_Out_Date_And_Time=null
												Total_Working_Time=0
												Worker_ID=Worker_Details[Worker_ID == val].ID
												Worker_Role=Worker_Details[Worker_ID == val].Worker_Role.Role
											];
										}
										var = Worker_Attendance[ID != null && Today_Date == zoho.currentdate];
										for each  rec in var
										{
											if(rec.Check_In_Date_And_Time == null && rec.Check_Out_Date_And_Time == null)
											{
												rec.Worker_Attendance_Status="Absent";
											}
											else if(rec.Check_In_Date_And_Time != null && rec.Check_Out_Date_And_Time != null)
											{
												rec.Worker_Attendance_Status="Present";
											}
											else if(rec.Check_In_Date_And_Time != null && rec.Check_Out_Date_And_Time == null)
											{
												rec.Worker_Attendance_Status="Check Out Missing";
											}
										}
					)
					}
				}
			}
		}




