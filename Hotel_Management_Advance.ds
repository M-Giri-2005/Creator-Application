								message : "<div>Your request has been approved.<br></div>"
							)
						}
						actions 
						{
						on load
						(
							input.Booking_ID = "BI" + "_" + input.Customer_Name.subString(0,3) + "_" + input.Auto_Number_4;
												// 
												// 
												ver = customer_details[ID == input.Customer_ID];
												if(ver.Customer_Visit != null)
												{
													ver.Customer_Visit=ver.Customer_Visit + 1;
												}
												else
												{
													ver.Customer_Visit=1;
												}
												for each  vicky in input.Room_Booking_Subform
												{
													vicky.Booking_Status="Confirmed";
													var = room_details[ID == vicky.Room_ID];
													var.Room_Book_or_Not="book The Room";
												}
												ver = insert into Payment_Collection
												[
													Added_User=zoho.loginuser
													Payment_Method="Cash"
													Advance_pay=input.Advance_Pay_2
													Payment_Status="Paid"
													Booking_ID=input.Booking_ID
												];
						)
						}
					}
					on reject
					{
						display name =  "Reject For Room Booking"
						actions 
						{
							sendmail
							(
								from : zoho.loginuserid
								to : Customer_Email
								subject : "Approval Request"
								message : "<div>Your request has been rejected.<br></div>"
							)
						}
						actions 
						{
						on load
						(
							input.Room_Booking_Subform.Booking_Status = "Cancelled";
						)
						}
					}
				}
			}
		}



