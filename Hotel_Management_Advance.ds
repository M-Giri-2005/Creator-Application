			Convert_Contacts as "Convert Contacts"
			{
				type =  functions
				form = customer_details
				execution type = for each record	
				on start
				{
					actions 
					{
					on click
					(
						new = customer_details[Customer_ID == input.Customer_ID];
										ver = new.CRM_ID;
										Contacts_values = Map();
										Contacts_values.put("First_Name ",input.Customer_Name.first_name);
										Contacts_values.put("Last_Name",input.Customer_Name.last_name);
