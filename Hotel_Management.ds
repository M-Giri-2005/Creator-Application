						custom deluge script
						(
											ver = Room_Details_master[ID == row.room_id];
										for each  rec in ver
										{
											row.room_price=ver.total_Price_per_day;
										}
										info ver;
						)
					}
				}

			}
			grand_total as "grand total"
			{
				type =  form
				form = Room_Booking
				record event = on add or edit

				on delete row of room_booking_all
				{
					actions 
					{
						custom deluge script
						(
											input.GRESS_GRAND_TOTAL = input.GRESS_GRAND_TOTAL - row.advance_amount;
										input.true_grand_total = input.true_grand_total - row.total_amount;
						)
					}
				}

			}
