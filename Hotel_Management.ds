			generate_the_room_booking as "generate the room booking id"
			{
				type =  form
				form = Room_Booking
				record event = on add

				on success
				{
					actions 
					{
						custom deluge script
						(
											input.booking_id = "BI" + "_" + "_" + input.Auto_Number_3;
										// new = customer_details_master[ID == input.customer_ID];
										// new.count_1=new.count_1 + 1;
										// // 
										// // 
										// // 
										ver = Room_Details_master[ID == input.room_booking_all];
										ver.room_Status1=input.room_booking_all.booking_status;
						)
					}
				}

			}
			CALCUALTE_THE_TOTAL_DAYS as "CALCUALTE THE TOTAL DAYS"
			{
				type =  form
				form = Room_Booking
				record event = on add or edit

				on user input of room_booking_all.check_out_date
				{
					actions 
					{
						custom deluge script
						(
											for each  new in input.room_booking_all
										{
											new.total_days=new.check_in_date.daysbetween(new.check_out_date);
											new.total_amount=new.total_days * new.room_price;
											new.advance_amount=new.total_amount / 2;
										}
										input.GRESS_GRAND_TOTAL = 0;
										for each  rec in input.room_booking_all
										{
											input.GRESS_GRAND_TOTAL = input.GRESS_GRAND_TOTAL + rec.advance_amount;
										}
										giri = customer_details_master[ID == input.customer_ID];
										if(giri.count_1 > 3)
										{
											input.DISCOUNT = input.GRESS_GRAND_TOTAL * 10 / 100;
											input.NET_GRAND_TOTAL = input.GRESS_GRAND_TOTAL - input.DISCOUNT;
										}
										else
										{
											input.NET_GRAND_TOTAL = input.GRESS_GRAND_TOTAL;
										}
										// 
										// 
										// 
										// 
										// 
										input.true_grand_total = 0;
										for each  rec in input.room_booking_all
										{
											input.true_grand_total = input.true_grand_total + rec.total_amount;
										}
										input.balance = input.true_grand_total - input.NET_GRAND_TOTAL;
						)
					}
				}

			}
			generate_the_customer_id as "generate the customer id"
			{
				type =  form
				form = customer_details_master
				record event = on add

				on success
				{
