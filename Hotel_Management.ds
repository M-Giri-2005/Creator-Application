				record event = on add or edit

				on user input of Amenities_types
				{
					actions 
					{
						custom deluge script
						(
											if(Amenities_types == "WIFI")
										{
											input.total_Price_per_day = input.Price_per_day + 500;
										}
										else if(Amenities_types == "TV")
										{
											input.total_Price_per_day = input.Price_per_day + 200;
										}
										else if(Amenities_types == "AC")
