			Fetch_Product_Price_calcu1 as "Fetch Product Price && calculate Total Price"
			{
				type =  form
				form = Sale_Product_To_Company
				record event = on add or edit

				on user input of Sales_Name_Product_List.Product_Name
				{
					actions 
					{
						custom deluge script
						(
											if(Time_Variation == "")
										{
											alert "Not Valid Reson For You First Enter The Time ";
											row.Product_Name=null;
										}
										// 
										// this is for Fetch The Product Price
										row.Price_Per_Liter=row.Product_Name.Product_Price;
										// 
										// this is the fetch The price In Different Product Name 
										rec = Today_Buy_Milk_Total[Product_Name == row.Product_Name.Product_Name];
										if(row.Product_Name.Product_Name == rec.Product_Name)
										{
											var = Today_Buy_Milk_Total[Product_Name == row.Product_Name.Product_Name && Today_Date == input.Sales_Date && Time_Variation == input.Time_Variation];
											row.Available_Stock=var.Total_Liter;
											row.Today_Total_Liter=var.Total_Liter;
											ver = Today_Buy_Milk_Total[Product_Name == row.Product_Name.Product_Name];
											// 
											// this is for Calculate the total Price in Price for liter
											row.Total_Liter_Today_Price=row.Today_Total_Liter * row.Price_Per_Liter;
											// 
											// this is for Calculate The total liter && total Price In today
										}
										input.Total_Liter = input.Sales_Name_Product_List.sum(Today_Total_Liter);
										input.total_Price = input.Sales_Name_Product_List.sum(Total_Liter_Today_Price);
										if(row.Product_Name == null && row.Today_Total_Liter == null)
										{
											row.Available_Stock=0;
											row.Today_Total_Liter=0;
											row.Price_Per_Liter=0;
											row.Total_Liter_Today_Price=0;
										}
										if(row.Product_Name == null)
										{
											input.total_Price = input.total_Price - row.Total_Liter_Today_Price;
											input.Total_Liter = input.Total_Liter - row.Today_Total_Liter;
											row.Available_Stock=0;
											row.Today_Total_Liter=0;
											row.Price_Per_Liter=0;
											row.Total_Liter_Today_Price=0;
										}
										if(row.Product_Name != null && row.Today_Total_Liter == null)
										{
											input.total_Price = input.total_Price - row.Total_Liter_Today_Price;
											input.Total_Liter = input.Total_Liter - row.Today_Total_Liter;
											row.Available_Stock=0;
											row.Today_Total_Liter=0;
											row.Price_Per_Liter=0;
											row.Total_Liter_Today_Price=0;
										}
						)
					}
				}

			}
			Calculate_The_Total_price2 as "Calculate The Total price"
			{
				type =  form
				form = Sale_Product_To_Company
				record event = on add or edit
					status = inactive

				on user input of Sales_Name_Product_List.Today_Total_Liter
				{
					actions 
					{
						custom deluge script
						(
											ver = Today_Buy_Milk_Total[Product_Name == row.Product_Name.Product_Name];
										if(row.Product_Name.Product_Name == ver.Product_Name)
										{
											if(row.Today_Total_Liter > row.Available_Stock)
											{
												alert "Not Valid Reson For No Stock For Today";
												row.Today_Total_Liter=0;
											}
										}
										if(row.Product_Name == null)
										{
											alert "Not Valid Reson For You Not Enter For Product Name";
											row.Today_Total_Liter=0;
										}
										if(row.Today_Total_Liter < 0)
										{
											alert "Not Valid Reson For You Enter For Nagetive Value";
											// 	input.Total_Price_Today+=row.Total_Liter_Today_Price;
											// 	input.Total_Liter+=row.Today_Total_Liter;
											row.Today_Total_Liter=0;
											row.Total_Liter_Today_Price=0;
										}
										if(row.Today_Total_Liter == null)
										{
											row.Today_Total_Liter=0;
										}
										// 
										// this is for Calculate the total Price in Price for liter
										row.Total_Liter_Today_Price=row.Today_Total_Liter * row.Price_Per_Liter;
										// 
										// this is for Calculate The total liter && total Price In today
										input.Total_Liter = input.Sales_Name_Product_List.sum(Today_Total_Liter);
										input.total_Price = input.Sales_Name_Product_List.sum(Total_Liter_Today_Price);
						)
					}
				}

