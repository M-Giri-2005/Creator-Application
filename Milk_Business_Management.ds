											// 	input.Total_Liter+=row.Today_Total_Liter;
											row.Today_Total_Liter=0;
											row.Total_Liter_Today_Price=0;
										}
										if(row.Today_Total_Liter == null)
										{
											row.Today_Total_Liter=0;
										}
										//
										// this is for Calculate the total Price in Price for liter
										if(row.Today_Total_Liter != null)
										{
											row.Total_Liter_Today_Price=row.Today_Total_Liter * row.Price_Per_Liter;
