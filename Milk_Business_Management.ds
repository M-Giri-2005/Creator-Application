		 functions
		 {
			 Deluge
			 {
				 string auto_E_mail(string username)
                 {
                 	if(username != null && username != "")
                 	{
                 		if(!username.contains("@"))
                 		{
                 			username = username + "@gmail.com";
                 		}
                 	}
                 	return username;
                 }
			 }
		 }
		workflow
		{
		form
		{
			Aleat_For_Negative_For_Va as "Aleat For Negative For Value"
			{
				type =  form
				form = PRODUCT_TYPE_LIST
				record event = on add or edit

				on user input of Product_Price
				{
					actions 
					{
						custom deluge script
						(
											if(Product_Price != null)
										{
											if(input.Product_Price < 0)
											{
												alert "Not Valid Reson For You Enter The Negative Value ";
												input.Product_Price = null;
											}
										}
						)
					}
				}

			}
			Generate_The_Customer_ID as "Generate The Customer ID"
			{
				type =  form
				form = Rural_Customer_And_Vendor_Details_Master
				record event = on add

				on success
				{
					actions 
					{
						custom deluge script
						(
											input.Customer_ID = "JD" + "_" + input.Customer_Type.subString(0,3) + "_" + zoho.currentdate.getYear() + "_" + input.Customer_Name.subString(0,3) + "_" + input.Auto_Number;
										if(Customer_Type == "Buy From Vendor")
										{
											vendor = Map();
											vendor.put("contact_name",input.Customer_ID);
											vendor.put("company_name",input.Customer_Name);
											vendor.put("contact_type","vendor");
											vendor_list = List();
											emtymap = Map();
											emtymap.put("email",input.Customer_Email);
											emtymap.put("phone",input.Customer_Phone);
											vendor_list.add(emtymap);
											vendor.put("contact_persons",vendor_list);
											response = invokeurl
											[
												url :"https://www.zohoapis.in/books/v3/contacts?organization_id=60047090525"
												type :POST
												parameters:vendor.tostring()
												connection:"zoho_books_connection"
											];
											var1 = response.get("contact").get("contact_id");
											input.Zoho_Book_ID = var1;
										}
										if(Customer_Type == "Sale To Customer")
										{
											customer_data = Map();
											customer_data.put("contact_name",input.Customer_ID);
											customer_data.put("company_name",input.Customer_Name);
											customer_data.put("contact_type","customer");
											Customer_list = List();
											emtymap = Map();
											emtymap.put("email",input.Customer_Email);
											emtymap.put("phone",input.Customer_Phone);
											Customer_list.add(emtymap);
											customer_data.put("contact_persons",Customer_list);
											response = invokeurl
											[
												url :"https://www.zohoapis.in/books/v3/contacts?organization_id=60047090525"
												type :POST
												parameters:customer_data.tostring()
												connection:"zoho_books_connection"
											];
											new1 = response.get("contact").get("contact_id");
											input.Zoho_Book_ID = new1;
											// 	info response;
										}
										// if(Customer_Type == "Sale To Customer")
										// {
