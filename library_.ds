		workflow
		{
		form
		{
			SELECT_THE_MEMBERSHIP_TYP as "SELECT THE MEMBERSHIP TYPE"
			{
				type =  form
				form = MEMBERSHIP_DETALIS
				record event = on add or edit

				on user input of MEMBERSHIP_TYPE
				{
					actions 
					{
						custom deluge script
						(
											ver = MEMBERSHIP_TYPES[MEMBERSHIP_TYPE == input.MEMBERSHIP_TYPE];
										if(MEMBERSHIP_TYPE == "GOLD")
										{
											input.MEMBER_ID = "G" + ver.MEMBER_ID + 1;
											input.MEMBERSHIP_PRICE = ver.MEMBER_PRICE;
											input.MEMBER_VALIDITY = ver.MEMBER_VALIDITY;
											input.max_count = ver.max_count;
										}
										else if(MEMBERSHIP_TYPE == "GOLD")
										{
											input.MEMBER_ID = "G" + ver.MEMBER_ID + 1;
											input.MEMBERSHIP_PRICE = ver.MEMBER_PRICE;
											input.MEMBER_VALIDITY = ver.MEMBER_VALIDITY;
											input.max_count = ver.max_count;
										}
										else if(MEMBERSHIP_TYPE == "GOLD")
										{
											input.MEMBER_ID = "G" + ver.MEMBER_ID + 1;
											input.MEMBERSHIP_PRICE = ver.MEMBER_PRICE;
											input.MEMBER_VALIDITY = ver.MEMBER_VALIDITY;
											input.max_count = ver.max_count;
										}
										else
										{
											input.MEMBER_ID = null;
											input.MEMBER_VALIDITY = null;
											input.MEMBERSHIP_PRICE = null;
										}
						)
					}
				}

			}
			UPDATE_FOR_MEMBER_ID as "UPDATE FOR MEMBER_ID"
			{
				type =  form
				form = MEMBERSHIP_DETALIS
				record event = on add or edit

				on success
				{
					actions 
					{
						custom deluge script
						(
											ver = MEMBERSHIP_TYPES[MEMBERSHIP_TYPE == input.MEMBERSHIP_TYPE];
										ver.MEMBER_ID=ver.MEMBER_ID + 1;
						)
					}
				}

			}
		}






