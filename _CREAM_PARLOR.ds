					{
						custom deluge script
						(
											var = ORDER_FOR_ICE_CREAM[ID == input.customer_ID];
										for each  ver in input.SubForm
										{
											for each  rec in var.ORDER_ITEM
											{
												if(rec.Ice_Cream_Flavors == ver.Ice_Cream_Flavors)
												{
													rec.Status=ver.Status;
												}
											}
										}
						)
					}
				}

			}
			Fetch_THE_ORDER_DETAILS as "Fetch THE ORDER DETAILS"
			{
				type =  form
				form = reorder_item
				record event = on add or edit

				on user input of customer_ID
				{
					actions 
					{
						custom deluge script
						(
											ver = ORDER_FOR_ICE_CREAM[ID == input.customer_ID];
										input.Name = ver.Name;
										input.you_choice = ver.Drop;
										input.Phone_Number = ver.Phone_Number;
										input.Checkbox = ver.Checkbox;
										input.GRAND_TOTAL = ver.GRAND_TOTAL;
										input.Address = ver.Address;
										for each  rec in ver.ORDER_ITEM
										{
											var = reorder_item.SubForm();
											var.Ice_Cream_Flavors=rec.Ice_Cream_Flavors;
											var.ice_toppings_list=rec.ice_toppings_list;
											var.Ice_Cream_price=rec.Ice_Cream_price;
											var.ice_toppings_price=rec.ice_toppings_price;
											var.ice_cream_total_price=rec.ice_cream_total_price;
											var.Status=rec.Status;
											input.SubForm.insert(var);
										}
						)
					}
				}

			}
			dynamic_clear as "dynamic clear"
			{
				type =  form
				form = ORDER_FOR_ICE_CREAM
				record event = on add or edit

				on add row of ORDER_ITEM
				{
					actions 
					{
						custom deluge script
						(
											ver = ICE_CREAM_DETAILS[ID != null].ID.getAll();
										new = List();
										for each  rec in input.ORDER_ITEM
										{
											new.add(rec.Ice_Cream_Flavors);
										}
										ver.removeall(new);
										row.Ice_Cream_Flavors:ui.add(ver);
						)
					}
				}

			}
		}






