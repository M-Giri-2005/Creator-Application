		workflow
		{
		form
		{
			calculate_the_ice_price as "calculate the ice price"
			{
				type =  form
				form = ORDER_FOR_ICE_CREAM
				record event = on add or edit

				on user input of ORDER_ITEM.Ice_Cream_Flavors
				{
					actions 
					{
						custom deluge script
						(
											ver = ICE_CREAM_DETAILS[ID == row.Ice_Cream_Flavors];
										row.Ice_Cream_price=ver.Ice_Cream_price;
						)
					}
				}

			}
			calculate_the_toppings_pr as "calculate the toppings price"
			{
				type =  form
				form = ORDER_FOR_ICE_CREAM
				record event = on add or edit

				on user input of ORDER_ITEM.ice_toppings_list
				{
					actions 
					{
						custom deluge script
						(
											ver = ice_toppings[ID == row.ice_toppings_list];
										input.ORDER_ITEM.ice_cream_total_price = ver.sum(ice_toppings_price);
										input.GRAND_TOTAL = 0;
										for each  rec in input.ORDER_ITEM
										{
											input.GRAND_TOTAL = input.GRAND_TOTAL + rec.ice_cream_total_price;
										}
										// count = 0;
										// for each  val in ver
										// {
										// 	count = count + val.ice_toppings_price;
										// }
										// row.ice_toppings_price=count;
										// row.ice_cream_total_price=row.Ice_Cream_price + row.ice_toppings_price;
										// input.GRAND_TOTAL = 0;
										// for each  rec in input.ORDER_ITEM
										// {
										// 	input.GRAND_TOTAL = input.GRAND_TOTAL + rec.ice_cream_total_price;
										// }
						)
					}
				}

