		approval
		{
			Travel_Approval as "Travel Approval"
			{
				type =  approval
				form = travel_Approvel[Travel_cost > 5000 && Request_status == "Pending"]
				display fields = [Request_ID,Request_Date,EMP_ID,EMP_NAME,Emp_Email,EMP_Phone,EMP_ROLE,Total_Travel_Date,Travel_cost]
				on start
				{
				}
				on level 1
				{
					approvers
					(
		 				role = "Manager"
					)
					on approve 
					{
		    			display name = "Approve The Travel"
						actions 
						{
							sendmail
							(
								from : zoho.loginuserid
								to : approver.submitter
								subject : "Approval Request"
								message : "Your request has been approved."
							)
						}
						actions 
						{
						on load
						(
							sendmail
												[
													from :zoho.adminuserid
													to :input.Emp_Email
													subject :"Your Travel Approval Status"
													message :"Your Request Is Approval"
												]
												input.Request_status = "Approval";
						)
						}
					}
					on reject
					{
						display name =  "Reject The  Travel"
						actions 
						{
							sendmail
							(
								from : zoho.loginuserid
								to : approver.submitter
								subject : "Approval Request"
								message : "Your request has been rejected."
							)
						}
						actions 
						{
						on load
						(
							sendmail
												[
													from :zoho.adminuserid
													to :input.Emp_Email
													subject :"Your Travel Approval Status"
													message :"Your Travel Apprioval Status Is Reject"
												]
												input.Request_status = "Reject";
						)
						}
					}
				}
			}
		}



