			CALCULATE_THE_Total_Data as "CALCULATE THE Total Data"
			{
				type =  form
				form = travel_Approvel
				record event = on add or edit

				on user input of From_Date
				{
					actions 
					{
						custom deluge script
						(
											if(input.To_Date != null)
										{
											if(input.From_Date != null)
											{
												var = input.From_Date.daysBetween(input.To_Date);
												input.Total_Travel_Date = var;
											}
										}
										else
										{
											input.Total_Travel_Date = 0;
										}
										if(input.From_Date < zoho.currentdate)
										{
											alert "Not Valid Reson For The From Date Is Past";
											input.From_Date = null;
											input.Total_Travel_Date = 0;
										}
						)
					}
				}

			}
			CALCULATE_THE_Total_Treva as "CALCULATE THE Total Treval Date"
			{
				type =  form
				form = travel_Approvel
				record event = on add or edit

				on user input of To_Date
				{
					actions 
					{
						custom deluge script
						(
											if(input.From_Date != null)
										{
											if(input.To_Date != null)
											{
												var = input.From_Date.daysBetween(input.To_Date);
												input.Total_Travel_Date = var;
											}
										}
										else
										{
											input.Total_Travel_Date = 0;
										}
										if(input.From_Date > input.To_Date)
										{
											alert "Not Valid Reson For The To Date Is Past";
											input.To_Date = null;
											input.Total_Travel_Date = 0;
										}
										if(input.From_Date == null)
										{
											if(input.To_Date != null)
											{
												alert "Not Valid Reson For You Frist Enter The From Date";
												input.To_Date = null;
												input.Total_Travel_Date = 0;
											}
										}
						)
					}
				}

			}
