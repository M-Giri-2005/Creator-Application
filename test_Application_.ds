					displayformat = plainnumber 
					linktodial = enable
				)
				Date_Of_Jouning as "Date Of Jouning"
				Emp_Address  as "Emp Address"
	 			(
	      			displayformat = [address_line_1+""+address_line_2+""+district_city+""+state_province+""+postal_Code+""+country]
	      			enable = Link_to_Map,Show_As_Text
	 			)
				Role as "Role "
				Salary
				Mark_Shreet as "12 Mark Shreet"
				Image
				Final_education as "Final education"
				degree_certificate as "degree certificate"
				diploma_certificate as "diploma certificate"
				Auto_Number as "Auto Number"
				Emp_Status as "Emp Status"
				custom action "Active"
				(
	  			workflow = Active
				show action in edit record menu = true
				column header = "Active"
				condition = (Emp_Status == "In Active")
				confirmation before execution
				(
					message for individual record = "Execute the Active action for this record?"
					confirm button text = "Yes"		
					cancel button text = "No"
				)
				)
				custom action "In Active "
				(
	  			workflow = In_Active
				show action in edit record menu = true
				column header = "In Active "
				condition = (Emp_Status == "Active")
				confirmation before execution
