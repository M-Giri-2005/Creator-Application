		workflow
		{
		form
		{
			hide_and_disable as "hide and disable"
			{
				type =  form
				form = customer_details_master
				record event = on add or edit

				field rules
				{
					actions 
					{
						hide customer_id,customer_age;
					}
					actions 
					{
						disable date_of_joning;
					}
				}

			}
			generate_the_customer_id as "generate the customer id"
			{
				type =  form
				form = customer_details_master
				record event = on add

				on success
				{
					actions 
					{
						custom deluge script
						(
											input.customer_id = "CUS" + "_" + zoho.currentdate.getYear() + "_" + input.Auto_Number_count;
						)
					}
				}

			}
			calculate_age as "calculate age"
			{
				type =  form
				form = customer_details_master
				record event = on add or edit

				on user input of customer_dob
				{
					actions 
					{
						custom deluge script
						(
											if(input.customer_dob != null)
										{
											if(input.customer_dob.yearsBetween(today) < 18)
											{
												input.customer_age = input.customer_dob.yearsBetween(today);
												input.account_age_type = "CHILD";
											}
											else
											{
												input.customer_age = input.customer_dob.yearsBetween(today);
												input.account_age_type = "ADULT";
												hide parent_number;
												hide parent_name;
												hide parent_details;
											}
										}
										else
										{
											input.customer_age = null;
											input.account_age_type = null;
										}
						)
					}
				}

			}
			generate_the_customer_id_ as "generate the customer id in saving acount"
			{
				type =  form
				form = opening_saving_account
				record event = on add

				on success
				{
					actions 
					{
