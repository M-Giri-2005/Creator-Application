		workflow
		{
		form
		{
			GET_TO_THE_BUS_TYPE_WITH_ as "GET TO THE BUS TYPE WITH UI.ADD"
			{
				type =  form
				form = PERSON_TICKET_BOOKING
				record event = on add or edit

				on user input of END_PLACE
				{
					actions 
					{
						custom deluge script
						(
											ver = BUS_NAMES_AND_ROTE[START_PLACE == input.START_PLACE.START_PLACE && END_PLACE == input.END_PLACE.END_PLACE];
										new = ver.BUS_TYPES.getall();
										input.BUS_TYPES:ui.add(new);
						)
					}
				}

			}
			SHARE_SEAT as "SHARE SEAT"
			{
				type =  form
				form = BUS_NAMES_AND_ROTE
				record event = on add or edit

				on user input of TOTAL_AVAILABLE_SEAT
				{
					actions 
					{
						custom deluge script
						(
											input.FRIST_CLASS_AVAILABLE_SEAT = input.TOTAL_AVAILABLE_SEAT / 100 * 40;
										input.SECOND_CLASS_AVAILABLE_SEAT = input.TOTAL_AVAILABLE_SEAT - input.FRIST_CLASS_AVAILABLE_SEAT;
						)
					}
				}

			}
			CALCULATE_THE_PRICE_IN_CH as "CALCULATE THE PRICE IN CHILD"
			{
				type =  form
				form = BUS_NAMES_AND_ROTE
				record event = on add or edit

				on user input of TICKET_PRICE
				{
					actions 
					{
						custom deluge script
						(
											input.TICKET_PRICE_FOR_CHILD = input.TICKET_PRICE / 2;
						)
					}
				}

			}
			CALCULATE_THE_TOTAL_PRICE as "CALCULATE THE TOTAL PRICE IN CHILD"
			{
				type =  form
				form = PERSON_TICKET_BOOKING
				record event = on add or edit

				on user input of TOTAL_BOOKING_SEAT_FOR_CHILD
				{
					actions 
					{
						custom deluge script
						(
											input.TOTAL_BUS_COST_IN_CHILD = input.BUS_PRICE_FOR_ONE_PERSON_CHILD.TICKET_PRICE_FOR_CHILD * input.TOTAL_BOOKING_SEAT_FOR_CHILD;
						)
					}
				}

			}
			CALCULATE_THE_TOTAL_BUS_C as "CALCULATE THE TOTAL BUS COST FOR AUDLT"
			{
				type =  form
				form = PERSON_TICKET_BOOKING
				record event = on add or edit

				on user input of TOTAL_BOOKING_SEAT_FOR_ADULT
				{
					actions 
					{
						custom deluge script
						(
											input.TOTAL_BUS_COST_IN_ADULT = input.BUS_PRICE_FOR_ONE_PERSON_ADULT.TICKET_PRICE * input.TOTAL_BOOKING_SEAT_FOR_ADULT;
						)
					}
				}

			}
			CALCULATE_THE_TOTAL_BUS_C1 as "CALCULATE THE TOTAL BUS COST FOR AUDLT AND CHILD"
			{
				type =  form
				form = PERSON_TICKET_BOOKING
				record event = on add or edit

				on user input of TOTAL_BUS_COST_IN_ADULT
				{
					actions 
					{
						custom deluge script
						(
											input.TOTAL_BUS_COST_IN_CHILD_AND_ADULT = input.TOTAL_BUS_COST_IN_ADULT + input.TOTAL_BUS_COST_IN_CHILD;
						)
					}
				}

			}
			GET_TO_THE_BUS_TYPE_WITH_1 as "GET TO THE BUS TYPE WITH UI.ADD"
			{
				type =  form
				form = APPLY_TO_THE_BUS_PASS
				record event = on add or edit

				on user input of END_PLACE
				{
					actions 
					{
						custom deluge script
						(
											ver = BUS_NAMES_AND_ROTE[START_PLACE == input.START_PLACE.START_PLACE && END_PLACE == input.END_PLACE.END_PLACE];
										new = ver.BUS_TYPES.getall();
										input.BUS_TYPES:ui.add(new);
						)
					}
				}

			}
		}






